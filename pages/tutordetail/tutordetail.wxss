.table{
  margin-top: 20rpx;
  display: table; 
  width: 100%; 
  border-collapse: collapse;
}

.table-tr {
  display: table-row; 
  }

  .table-ths {
     font-size: 30rpx; 
     display: table-cell;
     width: 40%;
     height: 60rpx;
     text-align: center;
     vertical-align: middle;
     word-break: keep-all;
     }

   .table-th {
    font-size: 30rpx; 
     display: table-cell;
     height: 60rpx;
      width: 60%;
     text-align: center;
     vertical-align: middle;
      word-break: break-all;
     }
     
     .table-item{
       margin-left: 40rpx;
      display: flex;
      flex-direction: row ;
     
     }

